{
  "start": [
    {
      "token": "namespace.member",
      "regex": "\\b(PerformanceMarketing|TrafficArbitrage|ABTesting|EmailMarketing|Debugging|EventDrivenDevelopment|Analysis|DataEngineering|DataWareHousing|GraphDataBases|DataVisual|GGPlot2|TidyVerse|Pandas|Amcharts|React|DuckDB|ElasticSearch|mySQL|Neptune|Tableau|PowerBI|Grafana|AzureMessagingQueue|AWSKafka|RabbitMQ|BusinessIntelligence|AdvancedDataAnalytics|CyberSecurity|DigitalMarketing_And_ECommerce|ProductManagement|Talent|PopARide|SelfEmployed|BusinessAnalyst|WebDeveloper|SeniorSoftwareEngineer|DataEngineerJuniorDeveloper|Meowdb|MetricFrame|Mylang|ResumeIDE)\\b"
    },

    {
      "token": "namespace.lang",
      "regex": "\\b(R|Python|Bash|PHP|JavaScript|TypeScript|Python|GremlinOpenCypher|CSS)\\b"
    },

    {
      "token": "namespace.qualifier",
      "regex": "\\b[A-Za-z_][A-Za-z0-9_]*\\b(?=::)"
    },
    {
      "token": "keyword.control",
      "regex": "\\b(enum|case|new)\\b"
    },

    {
      "token": "support.function",
      "regex": "\\b(work_experience|company|date_range|roles|about|skills|tools|education|certifications|personal_projects|major|school|accomplishments)\\b"
    },

    {
      "token": "constant.language",
      "regex": "\\b(Company|Title|Data|Product|DataViz|Lang|Lib|DB|Queue|Code|Google|Project)::[A-Za-z0-9_]+\\b"
    },

    {
      "token": "variable.language",
      "regex": "\\$(?:from|to)\\b"
    },

    {
      "token": "constant.language",
      "regex": "\\b(PRESENT)\\b"
    },

    {
      "token": "constant.numeric",
      "regex": "\\b\\d{4}_\\d{2}\\b"
    },

    {
      "token": "string",
      "regex": "\"[^\"]*\""
    },

    {
      "token": "comment.line.double-slash",
      "regex": "\\/\\/.*$"
    },
    {
      "token": "comment.block.documentation",
      "regex": "\\/\\*\\*[^]*?\\*\\/"
    },
    {
      "token": "comment.block",
      "regex": "\\/\\*[\\s\\S]*?\\*\\/"
    },

    {
      "token": "keyword.operator",
      "regex": "->|::|\\[|\\]|\\(|\\)|,|;"
    }
  ]
}
